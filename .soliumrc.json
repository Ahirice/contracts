{
  "extends": "solium:all",
  "plugins": [
    "security"
  ],
  "rules": {
    "security/no-assign-params": "off",
    "security/no-tx-origin": "off",
    "security/no-block-members": "off",
    "security/no-inline-assembly": "off",
    "arg-overflow": "off",
    "max-len": "off",
    "quotes": [
      "error",
      "double"
    ]
    ,
    "indentation": [
      "error",
      4
    ]
  }
}
